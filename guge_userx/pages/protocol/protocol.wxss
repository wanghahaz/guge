.protocol {
    width: 90%;
    margin: auto;
    box-shadow: 0 2rpx 9rpx 0 #EEEEEE;
    border-radius: 3rpx;
    font-size: 28rpx;
    margin-bottom: 40rpx;
}

p,
h1,
h3,
h4,
span {
    display: block;
}

p,
h3 {
    text-indent: 2em;
}

p,
span {
    margin-top: 20rpx;
}

h4 {
    text-align: center
}

h3,
h4 {
    font-size: 30rpx;
    margin-top: 20rpx;
    font-weight: 900;
}

h1 {
    text-align: center;
    font-weight: 900;
    margin: 30rpx 0;
    font-size: 40rpx;
}