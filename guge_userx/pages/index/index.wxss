.slide-image {
    height: 100%;
    width: 100%;
    border-radius: 20rpx;
    display: flex;
    flex-direction: column;
}

#blank {
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    left: 0;
    background: pink;
    height: 100%;
    width: 100%;
}

.slide-image .one {
    flex: 2;
    width: 100%;
    display: flex;
    flex-direction: column;
    font-family: MicrosoftYaHei;
    /* font-size: 35.99rpx; */
    font-weight: normal;
    font-stretch: normal;
    letter-spacing: 7.2rpx;
    color: #fefefe;
    text-align: center;
    justify-content: center;
}

.onetop {
    font-size: 43rpx;
}

.onebottom {
    margin-top: 37rpx;
    font-size: 40rpx;
}

.slide-image .two {
    flex: 1;
    width: 100%;
    background: #EEEEEF;
    border-radius: 0 0 20rpx 20rpx;
    display: flex;
    justify-content: space-around;
}

.twoleft,
.tworight {
    font-family: MicrosoftYaHei;
    font-size: 25rpx;
    font-weight: normal;
    font-stretch: normal;
    letter-spacing: 4.8rpx;
    color: #3e3a39;
    text-align: center;
    margin: auto;
    flex: 2;
}

a {
