/* 用户信息 */

.header_user {
    width: 100%;
    height: 160rpx;
    display: flex;
    padding: 30rpx 60rpx;
    /* background: pink; */
    box-sizing: border-box;
    align-items: flex-end;
    justify-content: space-between;
}

.header_user_left {
    display: flex;
    align-items: center;
}

.header_user_left image {
    width: 85rpx;
    height: 85rpx;
}

.header_user_left .userTx {
    width: 85rpx !important;
    height: 85rpx !important;
    border-radius: 50%;
    overflow: hidden;
}

.header_user_left>.usernames {
    min-width: 150rpx;
    height: 85rpx;
    display: flex;
    flex-direction: column;
    margin-left: 15rpx;
}

.header_user_left>.usernames>view {
    flex: 1;
    width: 100%;
    font-size: 24rpx;
    line-height: 42rpx;
    color: #c1d2d2;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.header_user_right {
    position: relative;
}

.header_user_right text {
    position: absolute;
    top: -5rpx;
    left: 18rpx;
    display: block;
    border-radius: 50%;
    width: 30rpx;
    height: 30rpx;
    line-height: 30rpx;
    text-align: center;
    font-size: 26rpx;
    color: wheat;
    background: red;
}

.header_user_right image:nth-child(1) {
    width: 33rpx;
    height: 36rpx;
    margin-right: 30rpx;
}

.header_user_right image:nth-child(2) {
    width: 38rpx;
    height: 36rpx;
    margin-right: 40rpx;
}

.header_user_right image:nth-child(3) {
    width: 42rpx;
    height: 39rpx;
}


